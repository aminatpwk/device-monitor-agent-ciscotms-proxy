{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RawMessageStore": {
    "Directory": "raw-messages"
  },
  "MessageProcessor": {
    "WorkerCount": 3
  },
  "C3": {
    "TelemetryEndpoint": "http://localhost:5000/api/telemetry",
    "ForwardingEnabled":  false
  }
}
